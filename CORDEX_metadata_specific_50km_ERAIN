sourceFILE[10]='EUROPE_STS'
#-- VARIABLE RegCM name
varalica[ 1]='t2avg'
varalica[ 2]='t2max'
varalica[ 3]='t2min'
varalica[ 4]='pcpavg'
varalica[ 5]='ps'
varalica[ 9]='sfcwind'
varalica[10]='w10max'



#-------- Global attributes
	Conventions="CF-1.4"
